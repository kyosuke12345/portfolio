{
  "name": "app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:create": "yarn typeorm migration:create -- -f ormconfig.js -n",
    "migration:generate": "yarn typeorm migration:generate -- -f ormconfig.js -n",
    "migration:run": "yarn typeorm migration:run -- -f ormconfig.js",
    "migration:revert": "yarn typeorm migration:revert -- -f ormconfig.js",
    "migration-heroku:run": "yarn typeorm migration:run -- -f ormconfig.heroku.js",
    "migration-heroku:revert": "yarn typeorm migration:revert -- -f ormconfig.heroku.js"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.6.1",
    "@nestjs/axios": "^0.0.7",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schedule": "^1.1.0",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^5.2.0",
    "@nestjs/typeorm": "^8.0.3",
    "@types/bcrypt": "^5.0.0",
    "@types/connect-redis": "^0.0.18",
    "@types/express-session": "^1.17.4",
    "@types/passport-local": "^1.0.34",
    "bcrypt": "^5.0.1",
    "cache-manager": "^4.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "connect-redis": "^6.1.1",
    "date-fns": "^2.28.0",
    "express-session": "^1.17.2",
    "helmet": "^5.0.2",
    "nodemailer": "^6.7.4",
    "passport": "^0.5.2",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3",
    "redis": "^3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "socket.io": "^4.5.1",
    "swagger-ui-express": "^4.3.0",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "type-guards": "^0.15.0",
    "typeorm": "^0.2.43",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@nestjs/platform-socket.io": "^9.0.3",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@nestjs/websockets": "^9.0.3",
    "@types/cache-manager": "^4.0.1",
    "@types/cron": "^1.7.3",
    "@types/date-fns": "^2.6.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.0",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.4",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@lib/lib(|/.*)$": "<rootDir>/libs/lib/src/$1"
    }
  }
}
